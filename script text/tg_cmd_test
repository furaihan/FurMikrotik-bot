:local parameter [[:parse [/system script get func_parameter source]] params=$params]
:put $parameter
:local param1 [:pick $parameter 0];
:local param2 [:pick $parameter 1];
:local param3 [:pick $parameter 2];
:local param4 [:pick $parameter 3];
:local param5 [:pick $parameter 4];

:local send [:parse [/system script get tg_sendMessage source]];
:local newline [:parse [/system script get func_newline source]];
:local test [$newline ("$param1\n$param2\n$param3\n$param4\n$param5")];
$send chat=$chatid text=$test